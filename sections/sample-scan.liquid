</section>

<script type="module">
  import ObjectPosition from '{{ "photoswipe-object-position.js" | asset_url }}';
  console.log(ObjectPosition);
  document.addEventListener("DOMContentLoaded", () => {
  const swiperOptions = {
    slidesPerView: 1,
    spaceBetween: 128,
    loop: true,
    autoplay: true,
    delay: 3000,
    navigation: {
      nextEl: "#next-slide",
      prevEl: "#prev-slide",
    },
    keyboard: {
      enabled: true,
      onlyInViewport: true,
      lazyPreloadPrevNext: 5,
    },
  };

  const swiperNegative = new Swiper("#swiper-negative-{{section.id}}", swiperOptions);
  const swiperPositive = new Swiper("#swiper-positive-{{section.id}}", swiperOptions);

  const createPhotoSwipeLightbox = (gallerySelector, swiperInstance) => {
    const photoSwipeOptions = {
      gallery: gallerySelector,
      pswpModule: PhotoSwipe,
      children: 'a',
      loop: true,
      showHideAnimationType: 'fade',
      mouseMovePan: true,
      initialZoomLevel: 'fit',
      secondaryZoomLevel: 1.5,
      maxZoomLevel: 2,
      wheelToZoom: true, 
    };

    const lightbox = new PhotoSwipeLightbox(photoSwipeOptions);
    new ObjectPosition(lightbox);
    lightbox.addFilter('useContentPlaceholder',
                   (useContentPlaceholder, content) => { return false; });

    lightbox.init();

    lightbox.on('change', () => {
      const { pswp } = lightbox;
      swiperInstance.slideTo(pswp.currIndex, 0, false);
      console.log('Slide index', pswp.currIndex);
    });

    lightbox.on('afterInit', () => {
      if (swiperInstance.params.autoplay && swiperInstance.params.autoplay.enabled) {
        swiperInstance.autoplay.stop();
      }
    });

    lightbox.on('closingAnimationStart', () => {
      const { pswp } = lightbox;
      swiperInstance.slideTo(pswp.currIndex, 0, false);
      if (swiperInstance.params.autoplay && swiperInstance.params.autoplay.enabled) {
        swiperInstance.autoplay.start();
      }
    });

    return lightbox;
  };

  const lightboxPositive = createPhotoSwipeLightbox('#swiper-positive-{{section.id}}', swiperPositive);
  const lightboxNegative = createPhotoSwipeLightbox('#swiper-negative-{{section.id}}', swiperNegative);
  });
</script>


<script>
  const toggle = document.querySelector("#negative_mode");
      const section = document.querySelector(".section-{{section.id}}")
  
      toggle.addEventListener("change", (e) => {
    section.classList.toggle("negative");

    if (e.target.checked) {
      section.classList.remove("color-{{ section.settings.color_scheme }}")
      section.classList.add("color-{{ section.settings.color_scheme_dark }}")
    } else {
      section.classList.add("color-{{ section.settings.color_scheme }}")
      section.classList.remove("color-{{ section.settings.color_scheme_dark }}")

    }
  });
</script>


