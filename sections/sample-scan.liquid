                      <span class="tw-uppercase tw-text-xs tw-font-bold">Film stock</span>
                      <span>{{ block.settings.film_stock }}</span>
                    </div>
                  {% endif %}

                </div>
              {% endunless %}
              <img
                srcset="{{ block.settings.sample_scan_positive | image_url: width: 1400 }} 1x, {{ block.settings.sample_scan_positive | image_url: width: 3840 }} 4x"
                sizes="1400px, 3840px"
                class="positive-{{block.id}}  tw-origin-center tw-object-cover tw-w-full tw-h-full" />
              <img
                srcset="{{ block.settings.sample_scan_negative | image_url: width: 1400 }} 1x, {{ block.settings.sample_scan_negative | image_url: width: 3840 }} 4x"
                sizes="1400px, 3840px"
                class="negative-{{block.id}} hidden  tw-origin-center tw-object-cover tw-w-full tw-h-full" />
              <div class="md:tw-hidden tw-p-6 tw-pb-0">
                {% unless block.settings.camera == blank and block.settings.lens == blank and block.settings.light_source == blank and block.settings.converted_with == blank and block.settings.film_stock == blank %}
                  <div class="tw-z-10 tw-grid tw-grid-cols-2 tw-gap-x-2 tw-gap-y-4">
                    {% if block.settings.camera != blank %}
                      <div class="tw-flex tw-flex-col tw-gap-1">
                        <span class="tw-uppercase tw-text-xs tw-font-bold">Scanning camera</span>
                        <span>{{ block.settings.camera }}</span>
                      </div>
                    {% endif %}
                    {% if block.settings.lens != blank %}
                      <div class="tw-flex tw-flex-col tw-gap-1">
                        <span class="tw-uppercase tw-text-xs tw-font-bold">Scanning lens</span>
                        <span>{{ block.settings.lens }}</span>
                      </div>
                    {% endif %}
                    {% if block.settings.light_source != blank %}
