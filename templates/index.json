    "image_with_text_tn7xXN": {
      "type": "image-with-text",
      "blocks": {
        "caption_fHMKdj": {
          "type": "caption",
          "settings": {
            "caption": "Professional grade tools",
            "text_style": "caption-with-letter-spacing",
