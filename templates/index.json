        "heading_CdjEd4": {
          "type": "heading",
          "settings": {
            "heading": "What is camera scanning?",
            "heading_size": "h1"
          }
        },
        "text_8WgbBb": {
          "type": "text",
          "settings": {
            "text": "<p>Camera scanning is the modern alternative to slow and bulky flatbed film scanners, or handing your negatives off to a dedicated lab. Simply use use a macro-capable digital camera to take a photo of the negative, then invert the colors to get a professional grade scans at home.<\/p><p><\/p>"
          }
        },
        "button_e8i3hQ": {
          "type": "button",
          "settings": {
            "button_label": "Intro to DSLR scanning",
            "button_link": "shopify:\/\/pages\/introduction-to-camera-scanninng",
            "button_style_secondary": false,
            "button_label_2": "",
            "button_link_2": "",
            "button_style_secondary_2": false
          }
        }
      },
      "block_order": [
